{
  "name": "scottspence.com",
  "version": "0.0.1",
  "description": "A digital garden.",
  "main": "index.js",
  "author": {
    "name": "Scott Spence",
    "email": "yo@scottspence.dev",
    "url": "https://scottspence.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop -p 4512 -o",
    "build": "gatsby build && yarn wellknown && yarn robots && yarn newsletters",
    "serve": "gatsby serve -p 4500 -o",
    "format": "pretty-quick",
    "wellknown": "cp -r .well-known/ public/",
    "robots": "cp robots.txt public/",
    "newsletters": "cp -r newsletters/ public/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
